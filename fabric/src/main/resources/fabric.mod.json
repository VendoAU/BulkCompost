{
	"schemaVersion": 1,
	"id": "${mod_id}",
	"version": "${version}",
	"name": "${mod_name}",
	"description": "${description}",
	"authors": [
		"${mod_author}"
	],
	"contact": {
		"homepage": "https://modrinth.com/mod/bulk-compost",
		"sources": "https://github.com/VendoAU/BulkCompost",
		"issues": "https://github.com/VendoAU/BulkCompost/issues"
	},
	"license": "${license}",
	"icon": "${mod_id}.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.vendoau.bulkcompost.BulkCompost"
		]
	},
	"mixins": [
		"${mod_id}.mixins.json",
		"${mod_id}.fabric.mixins.json"
	],
	"depends": {
		"fabricloader": ">=${fabric_loader_version}",
		"minecraft": "${fabric_minecraft_version_range}",
		"java": ">=${java_version}"
	}
}